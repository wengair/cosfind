
<div class="container">
  <% if params[:label] == 'character' %>
  <div class="row">
    <div class="add-char">
      <%= link_to new_character_path, class: "btn btn-secondary add_btn" do%>
       <i class="fas fa-plus-circle add_logo"></i>Add a Character
      <% end %>
    </div>
    <%= render 'characters/list_characters' %>
  </div>
  <% else %>
    <%= render 'bookings/index' %>
  <% end %>
</div>
