<h1>Characters#new</h1>
<p>Find me in app/views/characters/new.html.erb</p>
<%= simple_form_for(@character) do |f| %>
  <%= f.input :name%>
  <%= f.input :price%>
  <%= f.input :description%>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit "add a character", class: "btn btn-secondary" %>
<% end %>
